package com.algorithm;

import java.util.Scanner;

public class BackJun {
	
	public static void backJun(String[] args) {
		Scanner in = new Scanner(System.in);
		
		int A = in.nextInt();
		int B = in.nextInt();
		
		in.close();
		
		if(A>B) {
			System.out.println(">");
		} else if( A<B){
			System.out.println("<");
		} else {
			System.out.println("==");
		}
		
		
	}
}
